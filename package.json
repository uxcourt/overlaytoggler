{
  "name": "symbol-overlay-toggle",
  "description": "Find library-sourced symbols in the current page and render an overlay on them.",
  "version": "3.1.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "symbol-overlay-toggle",
    "manifest": "src/manifest.json",
    "main": "symbol-overlay-toggle.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uxcourt/overlaytoggler.git"
  }
}
